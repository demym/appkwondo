<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="33" id="com.tkdr.appkwondocrosswalk" version="1.2.8" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="AudioRecorderAPI">
        <param name="android-package" value="com.emj365.plugins.AudioRecorderAPI" />
    </feature>
    <feature name="Badge">
        <param name="android-package" value="de.appplant.cordova.plugin.badge.Badge" />
    </feature>
    <preference default="20+" name="xwalkVersion" />
    <preference default="xwalk_core_library_canary:17+" name="xwalkLiteVersion" />
    <preference default="--disable-pull-to-refresh-effect" name="xwalkCommandLine" />
    <preference default="embedded" name="xwalkMode" />
    <preference default="true" name="xwalkMultipleApk" />
    <preference default="16" name="android-minSdkVersion" />
    <feature name="Notification">
        <param name="android-package" value="org.apache.cordova.dialogs.Notification" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileOpener">
        <param name="android-package" value="fr.smile.cordova.fileopener.FileOpener" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="DeviceFeedback">
        <param name="android-package" value="cz.Velda.cordova.plugin.devicefeedback.DeviceFeedback" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SocialSharing">
        <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="LocalNotification">
        <param name="android-package" value="de.appplant.cordova.plugin.localnotification.LocalNotification" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Base64ImageSaverPlugin">
        <param name="android-package" value="org.solderzzc.Base64ImageSaverPlugin.Base64ImageSaverPlugin" />
    </feature>
    <name>AppKwonDoCross</name>
    <description>
       AppKwonDoRozzanoCordova
    </description>
    <author email="demy@taekwondorozzano.com" href="http://cordova.io">
        DemyKwonDo
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon src="www/img/logotkdrozzano_icon.png" />
    <icon density="ldpi" platform="android" src="www/img/logotkdrozzano_icon.png" />
    <icon density="mdpi" platform="android" src="www/img/logotkdrozzano_icon.png" />
    <icon density="hdpi" platform="android" src="www/img/logotkdrozzano_icon.png" />
    <icon density="xhdpi" platform="android" src="www/img/logotkdrozzano_icon.png" />
    <splash density="land-ldpi" src="resources/android/splash/screen.png" />
    <splash density="land-mdpi" src="resources/android/splash/screen.png" />
    <splash density="land-hdpi" src="resources/android/splash/screen.png" />
    <splash density="land-xhdpi" src="resources/android/splash/screen.png" />
    <splash density="land-xxhdpi" src="resources/android/splash/screen.png" />
    <splash density="land-xxxhdpi" src="resources/android/splash/screen.png" />
    <splash density="port-ldpi" src="resources/android/splash/screen.png" />
    <splash density="port-mdpi" src="resources/android/splash/screen.png" />
    <splash density="port-hdpi" src="resources/android/splash/screen.png" />
    <splash density="port-xhdpi" src="resources/android/splash/screen.png" />
    <splash density="port-xxhdpi" src="resources/android/splash/screen.png" />
    <splash density="port-xxxhdpi" src="resources/android/splash/screen.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="webView" value="org.crosswalk.engine.XWalkWebViewEngine" />
    <preference name="android-targetSdkVersion" value="19" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="xwalkVersion" value="20+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
    <preference name="android-minSdkVersion" value="16" />
</widget>
